PORT=3002
BASE_PATH=/questionnaire

# SuperAdmin login (required for /superadmin)
SUPERADMIN_EMAIL=admin@example.com
SUPERADMIN_PASSWORD=changeme
# SuperAdmin session expiry (default 30 min)
SUPERADMIN_JWT_EXPIRY=30m

# JWT secret (change in production)
JWT_SECRET=valentines-questionnaire-secret-change-in-production

# Google OAuth (optional, for admin "Sign in with Google")
# Create credentials at https://console.cloud.google.com/apis/credentials
# Add redirect URI: https://yourdomain.com/questionnaire/api/auth/google/callback
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# Connect to existing DB on host port 5433 (Docker maps 5433:5432)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/questionnaire
DB_HOST=localhost
DB_PORT=5433
DB_NAME=questionnaire
DB_USER=postgres
DB_PASSWORD=postgres
DATABASE_SSL=false
